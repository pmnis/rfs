<domain type='kvm'>
<name>Busybox2</name>
<memory unit='KiB'>1048576</memory>
<os>
<type>hvm</type>
<kernel>/boot/vmlinuz</kernel>
<initrd>/home/pmorel/VM/rfs00.gz</initrd>
<cmdline>console=ttyS0</cmdline>
</os>

<features>
<acpi/>
</features>

<devices>

